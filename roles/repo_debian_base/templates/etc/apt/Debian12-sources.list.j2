# {{ ansible_managed }}
# 2023092001

{% if repo_debian_base__mirror_url is defined and repo_debian_base__mirror_url | length %}
deb {{ repo_debian_base__mirror_url }}/debian/ {{ ansible_facts["distribution_release"] }} main
deb-src {{ repo_debian_base__mirror_url }}/debian/ {{ ansible_facts["distribution_release"] }} main

deb {{ repo_debian_base__mirror_url }}/debian-security {{ ansible_facts["distribution_release"] }}-security main
deb-src {{ repo_debian_base__mirror_url }}/debian-security {{ ansible_facts["distribution_release"] }}-security main

deb {{ repo_debian_base__mirror_url }}/debian/ {{ ansible_facts["distribution_release"] }}-updates main
deb-src {{ repo_debian_base__mirror_url }}/debian/ {{ ansible_facts["distribution_release"] }}-updates main

deb {{ repo_debian_base__mirror_url }} {{ ansible_facts["distribution_release"] }}-backports main
deb-src {{ repo_debian_base__mirror_url }} {{ ansible_facts["distribution_release"] }}-backports main
{% else %}
deb https://deb.debian.org/debian/ {{ ansible_facts["distribution_release"] }} main
deb-src https://deb.debian.org/debian/ {{ ansible_facts["distribution_release"] }} main

deb https://security.debian.org/debian-security {{ ansible_facts["distribution_release"] }}-security main
deb-src https://security.debian.org/debian-security {{ ansible_facts["distribution_release"] }}-security main

deb https://deb.debian.org/debian/ {{ ansible_facts["distribution_release"] }}-updates main
deb-src https://deb.debian.org/debian/ {{ ansible_facts["distribution_release"] }}-updates main

deb https://deb.debian.org/debian {{ ansible_facts["distribution_release"] }}-backports main
deb-src https://deb.debian.org/debian {{ ansible_facts["distribution_release"] }}-backports main
{% endif %}
