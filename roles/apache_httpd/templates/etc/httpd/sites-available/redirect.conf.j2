# {{ ansible_managed }}
# 2022062201
# Redirect vHost
{% if item['value']['by_role'] | d() %}
# Generated by Ansible role: {{ item['value']['by_role'] }}
{% endif %}

<VirtualHost {{ item['value']['virtualhost_ip'] | d('*') }}:{{ item['value']['virtualhost_port'] | d(80) }}>
    ServerName {{ item['value']['conf_server_name'] }}

    RewriteEngine On
{% if item['value']['raw'] | d() %}
# raw content
{{ item['value']['raw'] }}
{% else %}
    RewriteCond %{REQUEST_URI} !^/\.well\-known/acme\-challenge/
    RewriteRule ^(.*)$ https://%{HTTP_HOST}$1 [redirect=301,last,noescape]
{% endif %}
</VirtualHost>
